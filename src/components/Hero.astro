---
// import hexagons from "@data/hexagons-old.json"
import { Icon } from "astro-icon/components"
import ButtonLink from "./ButtonLink.astro"
import { getCollection } from "astro:content"

const hexagons = await getCollection("hexagons")

// enum HexTypes {
// 	NONE = 0,
// 	STROKE = 1,
// 	FILL = 2,
// 	LINK = 3
// }

// type Hexagon = {
// 	type: HexTypes
// 	alt?: string
// 	link?: string
// 	svg?: string
// }
---

<section class="mx-0 pb-12 sm:mx-4 md:mx-8 lg:mx-16" aria-labelledby="heading-hero">
	<div class="flex flex-col items-center">
		<h1 class="text-3xl font-semibold text-primary-600 md:text-4xl lg:text-5xl" id="heading-hero">
			Lighter. Faster. Better.
		</h1>
		<p class="mt-2 text-grayish-700 md:text-xl dark:text-grayish-100">
			Custom websites that work for <span class="font-bold">you</span>
		</p>
		<div class="mt-4 flex w-full items-center justify-center space-x-4">
			<ButtonLink href="/contact">Contact Me</ButtonLink>
			<ButtonLink href="#about" outline={true}>Learn more</ButtonLink>
		</div>
		<div class="mt-12 grid w-fit grid-cols-1 grid-rows-1 justify-items-center md:mt-16">
			<svg
				viewBox="0 0 1280 720"
				xmlns="http://www.w3.org/2000/svg"
				xmlns:xlink="http://www.w3.org/1999/xlink"
				xml:space="preserve"
				style="fill-rule:evenodd;clip-rule:evenodd;stroke-miterlimit:10"
				class="col-start-1 row-start-1 motion-safe:animate-[fadeIn_250ms_3000ms_ease-out_forwards] motion-safe:opacity-0"
				aria-label="Illustration of blob shape used as a background"
				><g transform="matrix(1.18794 0 0 -.95143 -69.216 785.973)"
					><path
						d="M1009.65 69.786c-54.875-25.78-683.035-60.303-819.308 0-71.787 31.767-11.251 84.309-22.562 261.324-4.429 69.316-55.305 133.542-79.613 178.153C77.389 529.044 57.91 591.03 58.27 627.89c.58 59.394 22.27 101.48 63 135.29 40.73 33.81 94.82 51.65 149.11 55.54 28.89 2.07 59.08.15 84.74-12.43 27.15-13.31 46.94-37.36 74.2-50.48 25.26-12.16 54.81-13.86 83.17-12.28 37.028 2.062 73.341 10.646 108.58 21.6 37.43 11.635 76.5 34.09 116 48.21 39.5 14.12 82.816 14.819 124.16 10.22 108.289-12.045 261.56-128.958 274.19-318.73 5.19-77.991-49.86-142.027-70.75-214.29-20.96-72.507 2.53-193.754-55.02-220.754Z"
						style="fill:url(#_RadialBlob);fill-rule:nonzero"
						id="Blob"></path></g
				>
				<defs
					><radialGradient
						id="_RadialBlob"
						cx="0"
						cy="0"
						r="0.98"
						gradientUnits="userSpaceOnUse"
						gradientTransform="matrix(-542.078 759.499 608.656 676.421 599.998 69.786)"
						><stop offset="0" style="stop-color:var(--color-blob-from);stop-opacity:1"></stop><stop
							offset=".43"
							style="stop-color:var(--color-blob-from);stop-opacity:.69"></stop><stop
							offset="1"
							style="stop-color:var(--color-blob-to);stop-opacity:0"></stop></radialGradient
					></defs
