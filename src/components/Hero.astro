												},
												{
													"motion-safe:animate-[fadeIn_250ms_3400ms_ease-out_forwards]":
														r == 1 && (c == 4 || c == 12)
												},
												{
													"motion-safe:animate-[fadeIn_250ms_3500ms_ease-out_forwards]":
														r == 1 && (c == 3 || c == 13)
												},
												{
													"motion-safe:animate-[fadeIn_250ms_3500ms_ease-out_forwards]":
