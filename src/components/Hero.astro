												},
												{
													"motion-safe:animate-[fadeIn_250ms_3375ms_ease-out_forwards]":
														(r == 0 || r == 2) && (c == 5 || c == 11)
												},
												{
													"motion-safe:animate-[fadeIn_250ms_3475ms_ease-out_forwards]":
														(r == 0 || r == 2) && (c == 4 || c == 12)
												},
												{
													"motion-safe:animate-[fadeIn_250ms_3575ms_ease-out_forwards]":
														(r == 0 || r == 2) && (c == 3 || c == 13)
												},
