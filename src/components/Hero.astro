												{ "bg-primary-600": hex.type === "fill" },
												{ "bg-transparent": hex.type === "stroke" },
												{ "bg-primary-50": hex.type === "icon" },
												{ "hover:scale-105": hex.type === "fill" },
												{ "hover:scale-110": hex.type === "icon" },
												{ "[&>a:focus-visible]:outline-none": hex.type === "icon" },
												{ "has-[a:focus-visible]:bg-white": hex.type === "icon" },
												{ "has-[a:focus-visible]:scale-150": hex.type === "icon" },
												{ "has-[a:focus-visible]:z-10": hex.type === "icon" },
												{
													"motion-safe:animate-[fadeIn_250ms_3000ms_ease-out_forwards]":
														r == 1 && c == 8
												},
												{
													"motion-safe:animate-[fadeIn_250ms_3100ms_ease-out_forwards]":
														r == 1 && (c == 7 || c == 9)
												},
												{
