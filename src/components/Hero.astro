												},
												{
													"motion-safe:animate-[fadeIn_250ms_3075ms_ease-out_forwards]":
														(r == 0 || r == 2) && c == 8
												},
												{
													"motion-safe:animate-[fadeIn_250ms_3175ms_ease-out_forwards]":
														(r == 0 || r == 2) && (c == 7 || c == 9)
												},
												{
													"motion-safe:animate-[fadeIn_250ms_3275ms_ease-out_forwards]":
